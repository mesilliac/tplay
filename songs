#!/bin/sh
./play "156[5678]56[5678]56[532~~~][.532]" $@
./play "123[1]2[3123123]{t-3}1#3[1]#[31#31#3]{t-1}13#[11~~~]{t-7}[#5432]123[4]5[#5#5#5#]{t3t1t7}" --bpm=120 --mode=minor $@
./play "[1~1~1236~6~6532~2~2345~5~543]" --loop=2 $@
./play 1####### $@
./play ~13131~13131~~~~ --swing --mode=minor --bpm=200 --loop=2 $@
./play 1232101~1232181~18716b581232151~ --bpm=200 --swing $@
./play [1230]{r+2} --loop=8 $@
./play [3210]{r-2} --transpose=24 --loop=8 $@
./play [[m11231123{r+2}11231123{r-4}11231123{r-1}11231123{r+3}]] --transpose=12 --loop=2 $@
./play [[[1~~5~~5~~5~~5~55~5555~1~0~0~]]] --loop=4 $@
